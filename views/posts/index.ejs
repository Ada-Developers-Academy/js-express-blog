<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <% posts.forEach(function( post ){ %>
      <div class="post-container">
        <h3 class="title"><%= post.title %></h3>
        <p class="body"><%= post.body %></p>
      </div>
    <% }); %>
  </body>
</html>
